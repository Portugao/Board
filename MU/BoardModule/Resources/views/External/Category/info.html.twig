{# Purpose of this template: Display item information for previewing from other modules #}
<dl id="category{{ category.getKey() }}">
<dt>{{ category|muboardmodule_formattedTitle|notifyFilters('board.filter_hooks.categories.filter')|safeHtml }}</dt>
{% if category.description is not empty %}<dd>{{ category.description }}</dd>{% endif %}
</dl>
