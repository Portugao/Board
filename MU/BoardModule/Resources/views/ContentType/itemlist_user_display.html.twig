{# Purpose of this template: Display users within an external context #}
{% for user in items %}
    <h3>{{ user|muboardmodule_formattedTitle }}</h3>
    <p><a href="{{ path('muboardmodule_user_display', {'id': user.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
